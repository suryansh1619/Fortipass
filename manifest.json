{
  "manifest_version": 3,
  "name": "FortiPass",
  "version": "1.0.0",
  "content_scripts": [
    {
      "matches": ["https://192.168.55.253/*"],
      "js": ["Content.js"]
    }],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "permissions": ["storage"]
}
